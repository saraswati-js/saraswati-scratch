{
  "name": "@saraswati-js/runner",
  "version": "1.0.1",
  "scripts": {
    "build": "next build && next export",
    "runtime:content": "content --count=50 --dir=./@saraswati-cache/posts",
    "runtime:images": "images --startdir=./@saraswati-cache/posts --destdir=doc-images --public=./public",
    "runtime:sidebar": "sidebar --from=@saraswati-cache/posts/ --to=./@saraswati-cache/sidebar.json",
    "runtime:cleanup": "rm -rf @saraswati-cache",
    "runtime:setup": "run-s runtime:cleanup runtime:content runtime:images runtime:sidebar",
    "dev": "next",
    "saraswati:dev": "run-p dev images",
    "modifier": "markdown-modifier",
    "serve": "serve out -p 3000",
    "start": "next"
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "@material-ui/icons": "^4.11.2",
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^10.0.3",
    "@saraswati-js/daemon-images": "^1.0.6",
    "@saraswati-js/daemon-sidebar": "^1.0.5",
    "@saraswati-js/demo-content": "^1.2.4",
    "bootstrap": "^4.6.0",
    "classnames": "2.2.6",
    "date-fns": "2.10.0",
    "get-video-id": "^3.2.0",
    "gray-matter": "4.0.2",
    "klaw": "^3.0.0",
    "next": "latest",
    "next-mdx-remote": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "react-bootstrap": "^1.5.2",
    "react-dom": "^17.0.1",
    "readdirp": "^3.5.0",
    "rehype-katex": "^4.0.0",
    "remark": "11.0.2",
    "remark-autolink-headings": "^6.0.1",
    "remark-html": "10.0.0",
    "remark-math": "3.0.1",
    "serve": "^11.3.2",
    "unist-util-visit": "^2.0.3"
  },
  "devDependencies": {
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^1.4.0"
  },
  "license": "MIT"
}
